@model IEnumerable<User>


<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">DateOfBirth</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.DateOfBirth</td>
                <td>
                    <a type="button" class="btn btn-sm btn-outline-secondary" asp-controller="User" asp-action="Update" asp-route-id="@item.Id">Update</a>
                    <a type="button" class="btn btn-sm btn-outline-secondary" asp-controller="User" asp-action="ConfirmDelete" asp-route-id="@item.Id">Delete</a>
                </td>   
            </tr>
        }
    </tbody>
</table>

<a type="button" class="btn btn-sm btn-outline-secondary" asp-controller="User" asp-action="Create">Create User</a>